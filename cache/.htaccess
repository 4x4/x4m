#DirectoryIndex index.php index.html index.html.var index.shtml index.cfm index.htm

RewriteEngine on

RewriteRule ^admin$ /admin.php [NC,L]

AddDefaultCharset utf-8

#Options -Indexes


RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^(.*\.)(js)$ jcompress.php?q=$1$2 [L,NC]
RewriteRule ^((.*)\.)(cjs)$ jcompress.php?m=$2 [L,NC] 
RewriteRule ^((.*)\.)(ejs)$ jcompress.php?e=$2 [L,NC]

RewriteCond %{REQUEST_FILENAME} /robots.txt$ [NC]
RewriteRule ^ /index.php?action=robots [L]

RewriteCond  %{REQUEST_URI} !^/xjs/_components/Xinha/*|index.html|admin.php|upload2.php|upload.php|image.php|license|tree_xml_server.php|upl.php|install.php|login.php|php.php|php2.php|jcompress.php|captcha.php|download.php|swfupload.php|^/backup/*|^/lz/*|^/media/*
RewriteCond  %{REQUEST_FILENAME} !\.(xls|rar|zip|tar|ico|jpg|jpeg|gif|png|htc|css|pdf|js|rar|html|xml|png|swf|JPG|JPEG|GIF|PNG)$
RewriteRule  ^(.*)$ _run.php
DirectoryIndex _run.php

#php_flag eaccelerator.enable off
#php_value session.bug_compat_42 1
#php_value session.bug_compat_warn 0
#php_value magic_quotes_gpc 1
#php_value magic_quotes_runtime 0
#php_value magic_quotes_sybase 0
#php_value variables_order EGPCS
#php_value upload_max_filesize 20M
#php_value register_globals 0
#php_value short_open_tag 1
#php_value display_errors 1
#php_value output_buffering 1
#php_value include_path .